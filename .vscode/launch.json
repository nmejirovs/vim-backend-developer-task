    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run 'start:dev'",
            "name": "vim-home-assignment dev",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PORT": "8080",
                "NOTIFICATION_SERVER_URL": "http://localhost:5001/",
                "LOG_LEVELS": "[\"log\", \"fatal\", \"error\", \"warn\", \"debug\", \"verbose\"]"
            }
        },
        {
            "command": "npm run 'start'",
            "name": "vim-home-assignment",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PORT": "8080",
                "NOTIFICATION_SERVER_URL": "http://localhost:5001/",
                "LOG_LEVELS": "[\"log\", \"fatal\", \"error\", \"warn\", \"debug\", \"verbose\"]"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch NestJS Application",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}\\dist\\main.js",
            "env": {
              "PORT": "8080",
                "NOTIFICATION_SERVER_URL": "http://localhost:5001/",
                "LOG_LEVELS": "[\"log\", \"fatal\", \"error\", \"warn\", \"debug\", \"verbose\"]"
            },
            "args": ["--host", "::1"]
          }
    ]
}